			<Method>
				<Name>findRecId</Name>
				<Source><![CDATA[
    /// <summary>
    /// Finds a record of <c>ITVTRentalOrderJour</c> table
    /// </summary>
    /// <param name = "_rentalOrderJourRecId">Record id of <c>ITVTRentalOrderJour</c> table</param>
    /// <param name = "_selectForUpdate">Specified whether selected record must be selected for update</param>
    /// <returns>A buffer of <c>ITVTRentalOrderJour</c> table</returns>
    public static ITVTRentalOrderJour findRecId(
        RefRecId        _rentalOrderJourRecId,
        boolean         _selectForUpdate = false)
    {
        ITVTRentalOrderJour rentalOrderJour;    

        select firstonly rentalOrderJour
            where rentalOrderJour.RecId == _rentalOrderJourRecId;

        if (_selectForUpdate)
        {
            rentalOrderJour.selectForUpdate(true);
        }

        return rentalOrderJour;
    }

]]></Source>
