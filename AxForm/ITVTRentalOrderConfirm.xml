                qbdsRentalOrderJour.addRange(fieldNum(ITVTRentalOrderJour, SalesId)).value(salesTable.SalesId);
            }

            if (element.args()
                && element.args().menuItemName() == menuItemDisplayStr(ITVTrentalOrderConfirm))
            {
                QueryBuildDataSource qbdsRentalOrderJour = ITVTRentalOrderJour_q.datasourceTable(tableNum(ITVTRentalOrderJour));
                qbdsRentalOrderJour.addRange(fieldNum(ITVTRentalOrderJour, SalesType)).value(SysQuery::valueNot(SalesType::Journal));
            }
        }

]]></Source>
