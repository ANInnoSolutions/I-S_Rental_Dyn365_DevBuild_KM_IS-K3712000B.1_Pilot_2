			<Method>
				<Name>canClose</Name>
				<Source><![CDATA[
    public boolean canClose()
    {
        boolean ret = true;

        if (element.closedOk())
        {
            if (!rentalLine_DS.validateWrite())
            {
                ret =  false;
            }
        }
        ret = ret && super();

        return ret;
    }

]]></Source>
			</Method>
			<Method>
				<Name>close</Name>
				<Source><![CDATA[
    public void close()
    {
        #Task

        if (element.closedOk() && rentalLine_DS.validateWrite())
        {
            rentalLine_DS.write();

            curRentalLine.dataSource().research(true);
        }

        super();
    }

]]></Source>
			</Method>
			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    public void init()
    {
        if (!element.args() || !element.args().caller())
        {
            super();
            element.close();
            return;
        }

        callerFormRun = element.args().caller();
        curRentalLine = element.args().record();

        super(); 
    }

]]></Source>
