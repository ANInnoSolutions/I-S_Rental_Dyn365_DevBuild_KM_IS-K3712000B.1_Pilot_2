			<Method>
				<Name>createAlternativeProductLines</Name>
				<Source><![CDATA[
    protected void createAlternativeProductLines()
    {
        ttsbegin;

        ITVTRentalLine rentalLineOrig = ITVTRentalLine::findInventTransId(inventTransferLineOrig.ITVTOriginInventTransId);

        if (originalLine.Qty != inventTransferLineOrig.QtyTransfer)
        {
            inventTransferLineOrig.selectForUpdate(true);
            inventTransferLineOrig.QtyTransfer = originalLine.Qty;
            inventTransferLineOrig.update();

            rentalLineOrig.selectForUpdate(true);
            rentalLineOrig.Qty = inventTransferLineOrig.QtyTransfer;
            rentalLineOrig.update();
        }

        ITVTRentalLine rentalLineNew;
        SalesTable salesTable = SalesTable::find(rentalLineOrig.SalesId);
        ITVTRentalLine rentalLinePrev = rentalLineOrig;
        InventTransferLine inventTransferLineNew;
        InventTransferLine inventTransferLinePrev = inventTransferLineOrig;
        InventTransferTable inventTransferTable = InventTransferTable::find(inventTransferLineOrig.TransferId);

        while select alternativeProduct
        {
            rentalLineNew.clear();
            rentalLineNew.initNewRentalLine(salesTable.SalesId, alternativeProduct.ItemId, alternativeProduct.Qty, alternativeProduct.UnitId, alternativeProduct.InventDimId);
            rentalLineNew.LineNum = ITVTRentalLine::getNextLineNum(rentalLinePrev);
            
            if (rentalLineNew.validateWrite())
            {
                rentalLineNew.insert();
            }

            if (rentalLineNew.RecId)
            {
                inventTransferLineNew.clear();
                inventTransferLineNew.initValue();
                inventTransferLineNew.ItemId = rentalLineNew.ItemId;
                inventTransferLineNew.InventDimId = rentalLineNew.InventDimId;
                inventTransferLineNew.initFromInventTable(InventTable::find(rentalLineNew.ItemId));
                inventTransferLineNew.initFromInventTransferTable(inventTransferTable, true);
                inventTransferLineNew.QtyTransfer = rentalLineNew.Qty;
                inventTransferLineNew.QtyRemainReceive = inventTransferLineNew.QtyTransfer;
                inventTransferLineNew.QtyRemainShip = inventTransferLineNew.QtyTransfer;
                if (inventTransferLineNew.ShipDate < DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone()))
                {
                    inventTransferLineNew.ShipDate = DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone());
                }
                inventTransferLineNew.defaultField(fieldNum(InventTransferLine, ReceiveDate));

                inventTransferLineNew.ITVTOriginInventTransId = rentalLineNew.InventTransId;

                inventTransferLineNew.LineNum = InventTransferLine::getNextLineNum(inventTransferLinePrev);

                if (inventTransferLineNew.validateWrite())
                {
                    inventTransferLineNew.insert();
                }
            }

            buf2Buf(rentalLineNew, rentalLinePrev);
            buf2Buf(inventTransferLineNew, inventTransferLinePrev);
        }

        ttscommit;

        FormObjectSet ds = FormDataUtil::getFormDataSource(element.args().record());
        
        if (ds)
        {
            ds.reread();
            ds.research(true);
        }
    }

]]></Source>
