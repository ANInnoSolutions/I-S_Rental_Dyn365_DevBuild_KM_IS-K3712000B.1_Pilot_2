			<Method>
				<Name>init</Name>
				<Source><![CDATA[
    void init()
    {
        super();

        if (!element.args() || !element.args().caller())
        {
            throw error("@SYS22539");
        }

        inventTransferLineOrig = element.args().record();

        element.updateDesign(InventDimFormDesignUpdate::Init);
        element.setData();
    }

]]></Source>
			</Method>
			<Method>
				<Name>setData</Name>
				<Source><![CDATA[
    protected void setData()
    {
        alternativeProductTmp.ItemId = inventTransferLineOrig.ItemId;
        alternativeProductTmp.InventDimId = inventTransferLineOrig.InventDimId;
        alternativeProductTmp.Qty = inventTransferLineOrig.QtyTransfer;
        alternativeProductTmp.UnitId = inventTransferLineOrig.UnitId;

        alternativeProductTmp.insert();

        originalLine.setTmpData(alternativeProductTmp);
    }

]]></Source>
			</Method>
			<Method>
				<Name>updateDesign</Name>
				<Source><![CDATA[
    void updateDesign(InventDimFormDesignUpdate mode)
    {
        InventDimParm inventDimParmVisible;

        switch (mode)
        {
            case InventDimFormDesignUpdate::Init:
                if (!inventDimFormSetup)
                {
                    inventDimFormSetup = InventDimCtrl_Frm_EditDimensions::newFromForm(element);
                    inventDimFormSetup.parmSkipOnHandLookUp(true);
                    inventDimFormSetup.parmDimParmVisibleGrid(inventDimParmVisible);
                }
                break;

            case InventDimFormDesignUpdate::Active:
                inventDimFormSetup.formActiveSetup(InventDimGroupSetup::newItemId(AlternativeProduct.ItemId));
                inventDimFormSetup.formSetControls(true);
                break;

            case InventDimFormDesignUpdate::FieldChange:
                inventDimFormSetup.formActiveSetup(InventDimGroupSetup::newItemId(AlternativeProduct.ItemId));
                InventDimAlternativeProduct.clearNotSelectedDim(inventDimFormSetup.parmDimParmEnabled());
                inventDimFormSetup.formSetControls(true);
                break;

            default :
                throw error(strFmt("@SYS54195",funcName()));
        }
    }

]]></Source>
